[run]
source = src/gitprompt
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */env/*
    */.*
    */migrations/*
    */alembic/*
    */conftest.py
    */setup.py
    */build/*
    */dist/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = GitPrompt Coverage Report

[xml]
output = coverage.xml
